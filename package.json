        "axios": "^1.7.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.35.3",
        "electron-updater": "^6.1.8",
        "express": "^4.21.0",
        "framer-motion": "^11.9.0",
        "is-hotkey": "^0.2.0",
        "llamaindex": "^0.6.15",
        "lru-cache": "^11.0.1",
        "lucide-react": "^0.446.0",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.1.7",
        "openai": "^4.67.0",
        "slate": "^0.103.0",
