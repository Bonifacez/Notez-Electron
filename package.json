{
    "name": "Notez",
    "version": "0.1.5",
    "main": "dist-electron/main/index.js",
    "description": "An AI powered Markdown editor",
    "author": "Boniface",
    "private": true,
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:api": "nodemon --exec tsx src-api/start.ts",
